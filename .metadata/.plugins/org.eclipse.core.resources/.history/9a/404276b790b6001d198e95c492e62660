package lesson015.veteriner;

public class Kedi extends Hayvan {

	private String tur;
	private String cins;
	private String karne;

	public Kedi(String tur, String cins, String karne, boolean kayitliMi, String dogumTarihi) {
		super(dogumTarihi, kayitliMi, dogumTarihi);
		this.tur = tur;
		this.cins = cins;
	}

	public String getTur() {
		return tur;
	}

	public void setTur(String tur) {
		this.tur = tur;
	}

	public String getCins() {
		return cins;
	}

	public void setCins(String cins) {
		this.cins = cins;
	}

	@Override
	public String toString() {
		return "Kedi [getTur()=" + getTur() + ", getCins()=" + getCins() + ", isKayitliMi()=" + isKayitliMi()
				+ ", getDogumTarihi()=" + getDogumTarihi() + "]";
	}

}
