package lesson012;

public class Pizza {
	
	
	private int price;
	private boolean isVeg;
	
	Public Pizza() {
		
	}
	Public Pizza(boolean isVegie) {
			
	}
		
	
	
	public int getPrice() {
		return price;
	}
	public void setPrice(int price) {
		this.price = price;
	}
	public boolean isNormal() {
		return isNormal;
	}
	public void setNormal(boolean isNormal) {
		this.isNormal = isNormal;
	}
}
