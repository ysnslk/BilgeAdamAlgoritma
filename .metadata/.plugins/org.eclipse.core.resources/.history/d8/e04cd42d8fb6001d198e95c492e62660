package lesson015.veteriner;

public abstract class Hayvan {

	private boolean kayitliMi;
	private String dogumTarihi;
	
	
	
	
	public Hayvan(boolean kayitliMi, String dogumTarihi) {
		super();
		this.kayitliMi = kayitliMi;
		this.dogumTarihi = dogumTarihi;
	}
	public boolean isKayitliMi() {
		return kayitliMi;
	}
	public void setKayitliMi(boolean kayitliMi) {
		this.kayitliMi = kayitliMi;
	}
	public String getDogumTarihi() {
		return dogumTarihi;
	}
	public void setDogumTarihi(String dogumTarihi) {
		this.dogumTarihi = dogumTarihi;
	}
	@Override
	public String toString() {
		return "Hayvan [kayitliMi=" + kayitliMi + ", dogumTarihi=" + dogumTarihi + "]";
	}
	
	
}
