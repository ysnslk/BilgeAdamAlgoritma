package maraton001;

public class Question1 {

	public static void yildizYazdir() {
		for (int i = 0; i < 12; i++) {
			if (i7 8) {
				for (int j = 1; j < 7; j++) {
					System.out.print("* ");
				}
				System.out.println();

			} else {
				for (int j = 1; j < i; j++) {
					System.out.print("* ");
				}
				System.out.println();
			}
			
		}
	}

	public static void main(String[] args) {
		yildizYazdir();

	}

}
