package lesson003;

import java.util.Scanner;

public class Question29 {

	public static void main(String[] args) {
		//Kullanıcı sıfıra basana kadar girilen sayıları çarp
		Scanner sc = new Scanner(System.in);
		do {
			System.out.println("Sayı Giriniz");
			int sayi = sc.nextInt()
		} while (sayi!=0);
	}

}
