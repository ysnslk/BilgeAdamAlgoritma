package lesson004;

public class Question34 {

	public static void main(String[] args) {
		// Verilen kelimenin harflerini alt alta yazdıralrım.
		//java
		char b;
		String kelime = "java";
		for(int i=0;i<kelime.length();i++) {
			b= kelime.charAt(i);
			System.out.println(b);
		}
		
		
		//Kullanıcıdan string değeri alalım. 
		//harfleri yazdır.
		
		
	}

}
