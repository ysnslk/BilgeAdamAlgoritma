package lesson015.veteriner;

public class Kopek extends Hayvan {

	private int asiSayisi;
	private String cins;
	private String tur;
	private String karne;

	public Kopek(int asiSayisi, String cins, String tur, String karne, boolean kayitliMi, String dogumTarihi) {
		super(dogumTarihi, kayitliMi, dogumTarihi);
		this.asiSayisi = asiSayisi;
		this.cins = cins;
		this.tur = tur;
		this.karne = karne;
	}

	public int getAsiSayisi() {
		return asiSayisi;
	}

	public void setAsiSayisi(int asiSayisi) {
		this.asiSayisi = asiSayisi;
	}

	public String getCins() {
		return cins;
	}

	public void setCins(String cins) {
		this.cins = cins;
	}

	public String getTur() {
		return tur;
	}

	public void setTur(String tur) {
		this.tur = tur;
	}

	public String getKarne() {
		return karne;
	}

	public void setKarne(String karne) {
		this.karne = karne;
	}

	

}
