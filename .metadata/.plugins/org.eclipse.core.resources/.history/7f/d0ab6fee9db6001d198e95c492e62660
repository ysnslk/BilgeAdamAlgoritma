package lesson015.veteriner;

import java.util.ArrayList;

public class Musteri extends Insan {

	private String adres;
	private ArrayList<Hayvan>Hayvalar;

	public Musteri(String adres, String tcKimlik, String isim) {
		super(tcKimlik, isim);
		this.adres = adres;
	}

	public ArrayList<String> getHayvalar() {
		return Hayvalar;
	}

	public void setHayvalar(ArrayList<String> hayvalar) {
		Hayvalar = hayvalar;
	}

	public String getAdres() {
		return adres;
	}

	public void setAdres(String adres) {
		this.adres = adres;
	}

	

	@Override
	public void bilgileriGoster() {
		System.out.println(toString());
	}

	@Override
	public String toString() {
		return "Musteri [getHayvalar()=" + getHayvalar() + ", getAdres()=" + getAdres() + ", getTcKimlik()="
				+ getTcKimlik() + ", getIsim()=" + getIsim() + "]";
	}
	
	

}
