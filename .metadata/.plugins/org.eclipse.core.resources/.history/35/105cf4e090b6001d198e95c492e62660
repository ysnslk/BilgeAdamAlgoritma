package lesson015.veteriner;

public class Kopek extends Hayvan {

	private int asiSayisi;
	private String cins;
	private String tur;
	private String karne;

	public Kopek(int asiSayisi, String cins,String tur, boolean kayitliMi, String dogumTarihi) {
		super(dogumTarihi, kayitliMi, dogumTarihi);
		this.asiSayisi = asiSayisi;
		this.cins = cins;
	}

	public int getAsiSayisi() {
		return asiSayisi;
	}

	public void setAsiSayisi(int asiSayisi) {
		this.asiSayisi = asiSayisi;
	}

	public String getCins() {
		return cins;
	}

	public void setCins(String cins) {
		this.cins = cins;
	}

	@Override
	public String toString() {
		return "Kopek [getAsiSayisi()=" + getAsiSayisi() + ", getCins()=" + getCins() + ", isKayitliMi()="
				+ isKayitliMi() + ", getDogumTarihi()=" + getDogumTarihi() + "]";
	}

}
