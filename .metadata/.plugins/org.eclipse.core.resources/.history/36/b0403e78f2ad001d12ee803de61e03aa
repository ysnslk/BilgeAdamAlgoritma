package lesson010;

public class Profile {

	// attribute , property ,field
	// Gerekli attributeleri tanımlayalım
	
	public String uName;
	public String name;
	public String description;
	public String profilePicture;
	public int follows;
	public int followers;
	public String posts;
	public String stories;
	
	public void getProfile() {
		
	}

	@Override
	public String toString() {
		return "Profil : Kullanıcı Adı : " + uName + ", İsim : " + name + ", Açıklama : " + description + ", profilePicture="
				+ profilePicture + ", Takip Edilen : " + follows + ", Takipçiler : " + followers + ", Gönderiler : " + posts
				+ ", Hikayeler : " + stories ;
	}

}
